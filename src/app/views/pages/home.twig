{% extends "layout.html" %}
{% block content %}
    <table class="table table-striped table-bordered" cellspacing="0" width="100%">
        <th>id_ncr</th>
        <th>customer_ncr</th>
        <th>date_creation</th>
        {% for result in results %}
            <tr>
                <td>{{ result['id_ncr']}}</td>
                <td>{{ result['customer_ncr']}}</td>
                <td>{{ result['date_creation']}}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}

